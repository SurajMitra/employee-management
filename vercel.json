{
    "version": 2,
    "builds": [
      { "src": "dist/employee-management1/**", "use": "@vercel/static" },
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server.js" },
      { "src": "/(.*)", "dest": "/dist/employee-management1/index.html" }
    ]
  }
  